$("#upload_status").html("Uploading documents: <%=@upload_count%>")
$("#backup_status").html("Backup documents: <%=@backup_count%>")

<% if @backup_status!="ok" %>
        $("#backup_status").css({'background-color':'red'}).animate({'background-color':'yellow'}, 1000);
        $("#backup_status").html("Backup documents: <%=@backup_count%> with status <%=@backup_status%>")
<%end%>

<% @converted_pages.each do |page| %>
    $("#sortable1 #page_"+<%= page.id %>).replaceWith("<%= escape_javascript(render :partial => 'page_uploaded', :locals => {:page => page}) %>");
<% end %>

